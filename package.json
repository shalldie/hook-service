{
  "name": "hook-service",
  "version": "1.0.0",
  "packageManager": "pnpm@7.11.0",
  "private": true,
  "description": "Monorepo for service„ÄÅstate manager to Vue/React.",
  "scripts": {
    "clean": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "dev:vue": "pnpm -r --filter=vue-hook-svc dev",
    "dev:react": "pnpm -r --filter=react-hook-svc dev",
    "build": "pnpm -r build",
    "patch": "pnpm -r exec npm version patch --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shalldie/hook-service.git"
  },
  "keywords": [
    "react",
